/**
 * Exclusive-or gate:
 * out = not (a == b)
 */

CHIP Xor {
    IN a, b;
    OUT out;

    PARTS:
    Nand(a=a, b=b, out=nand);
    Nand(a=a, b=nand, out=nanda);
    Nand(a=b, b=nand, out=nandb);
    Nand(a=nanda, b=nandb, out=out);
}